{
    "name": "jolicode/harvest-api-doc-extractor",
    "description": "Generate a Harvest OpenAPI 2.0 / Swagger specification from the api documentation",
    "type": "library",
    "keywords": ["harvestapp", "harvest", "swagger", "openapi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Xavier Lacot",
            "email": "xlacot@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoliCode\\Extractor\\": "src"
        }
    },
    "require": {
        "symfony/console": "^7.2.0",
        "symfony/dom-crawler": "^7.2.0",
        "symfony/css-selector": "^7.2.0",
        "symfony/yaml": "^7.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.65.0"
    }
}
